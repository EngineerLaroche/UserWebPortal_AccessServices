{
  "name": "LOG-210_Lab",
  "version": "0.1.0",
  "private": true,
  "main": "/src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "aws s3 sync build/ s3://wonderful-app",
    "test:watch": "jest --watch --coverage",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": "8.12.0"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^24.9.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupEnzyme.js",
    "coverageReporters": [
      "text"
    ],
    "reporters": [
      "default"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Augerr/LOG-210_Lab"
  },
  "dependencies": {
    "ag-grid-community": "^19.1.4",
    "ag-grid-react": "^19.1.2",
    "ajv": "^6.10.2",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.4.1",
    "enzyme-to-json": "^3.4.3",
    "history": "^4.10.1",
    "i18n": "^0.8.4",
    "i18n-express": "^1.1.3",
    "intl": "^1.2.5",
    "jquery": "^3.4.1",
    "marked": "^0.8.0",
    "moment": "^2.24.0",
    "nyc": "^15.0.0",
    "popper": "^1.0.1",
    "popper.js": "^1.16.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "propper": "^1.0.3",
    "proptypes": "^1.1.0",
    "react": "^16.12.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.3.0",
    "react-datepicker": "^1.8.0",
    "react-dom-factories": "^1.0.2",
    "react-intl": "^2.9.0",
    "react-redux": "^5.1.2",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-select": "^2.4.4",
    "react-time-picker": "^3.9.0",
    "react-translate": "^6.1.0",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.1",
    "superagent": "^3.8.2",
    "superagent-promise": "^1.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
